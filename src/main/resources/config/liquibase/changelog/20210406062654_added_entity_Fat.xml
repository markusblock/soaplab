            </column>
            <column name="inci" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="sap_naoh" type="double">
                <constraints nullable="true" />
            </column>
            <column name="sap_koh" type="double">
                <constraints nullable="true" />
            </column>
            <column name="lauric" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="myristic" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="palmitic" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="stearic" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="ricinoleic" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="oleic" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="linoleic" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="linolenic" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="iodine" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="ins" type="integer">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
    </changeSet>

    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here -->
</databaseChangeLog>
